$TTL 86400
@ IN SOA dns-primary.tesrov.lab. admin@tesrov.lab. (
		2020011800 ;Serial
		3600 ;Refresh
		1800 ;Retry
		604800 ;Expire
		86400 ;Minimum TTL
)
